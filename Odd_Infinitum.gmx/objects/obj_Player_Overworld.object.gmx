        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Reads input from keyboard

// Movement controls
key_up = keyboard_check(vk_up) || (gamepad_axis_value(0,gp_axislv) &lt; 0);
key_down = keyboard_check(vk_down) || (gamepad_axis_value(0,gp_axislv) &gt; 0);
key_left = keyboard_check(vk_left) || (gamepad_axis_value(0,gp_axislh) &lt; 0);
key_right = keyboard_check(vk_right) || (gamepad_axis_value(0,gp_axislh) &gt; 0);
key_inventory = keyboard_check_pressed(vk_shift) || (gamepad_button_check_pressed(0, gp_shoulderr));

// Read movement controls
if (!inventory_open &amp;&amp; !menu)
{
    if (key_left and x &gt; 0) 
    {
        x -= overworld_speed; 
        if (instance_exists(obj_Server))
            write_buffer_SERVER(0);
        if (instance_exists(obj_Client))
            write_buffer_CLIENT(0);
    }
    if (key_right and x &lt;= room_width - 56) 
    {
        x += overworld_speed;
        if (instance_exists(obj_Server))
            write_buffer_SERVER(0);
        if (instance_exists(obj_Client))
            write_buffer_CLIENT(0);
    }
    if (key_up and y &gt; 0)
    {
        y -= overworld_speed; 
        if (instance_exists(obj_Server))
            write_buffer_SERVER(0);
        if (instance_exists(obj_Client))
            write_buffer_CLIENT(0);
    }
    if (key_down and y &lt;= room_height - 32) 
    {
        y += overworld_speed;
        if (instance_exists(obj_Server))
            write_buffer_SERVER(0);
        if (instance_exists(obj_Client))
            write_buffer_CLIENT(0);
    }
}

// Callouts
check_callout();

// Open Inventory
if (key_inventory &amp;&amp; !inventory_open)
    inventory_open = true;
else if (key_inventory &amp;&amp; inventory_open)
    inventory_open = false;

</string>
