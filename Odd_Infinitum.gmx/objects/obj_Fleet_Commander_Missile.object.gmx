    effect_create_above(ef_smoke, x+42, y, 0, c_white);

    // Thrust 
    motion_set(180, thrust_speed);
    if (thrust_speed &gt; 2)
        thrust_speed -= 0.025;
}
else
{
    // Set X and Y to Fleet Commander Position
    x = obj_Fleet_Commander_Daemeon.x-112;
    y = obj_Fleet_Commander_Daemeon.y;
}
