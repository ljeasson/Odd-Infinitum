            <string>/// Move in direction of player

if (follow)
{
    // Move towards player
    move_towards_point(obj_Player.x, obj_Player.y, 2);

    // Face towards player
    image_angle = point_direction(x, y, obj_Player.x, obj_Player.y) + 180;
}
else if (thrust)
{
    // Blast smoke
    if (!thrust_blast)
