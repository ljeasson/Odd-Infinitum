          <argument>
            <kind>1</kind>
            <string>/// Combat

//When health is zero, boss is defeated
if (boss_health &lt; 0) {boss_health = 0;}
if (boss_health == 0) {defeated = true;}

// While Not Defeated (i.e. In Combat)
if (!defeated)
{
    if (combat)
    {
        // Randomly Generate a Large Red Laser
        if (attack_lock == false)
        {
            var laser = instance_create(x,y,obj_Red_Large_Laser_Controller);             
            
            attack_lock = true;
            alarm[0] = random_range(240,360);
        }
    
        // Generate Red Spawner Orb
        if (instance_number(obj_Red_Spawner_Glow))
            alarm[3] = 180; 
    }
}
else
{
    // Stop Moving
    path_end();
    
    // Generate Random Explosions
    generate_explosions(99);
    
    // Set Explosion Timer
    if (!explosion_imminent)
    {
        alarm[2] = 180;
        explosion_imminent = true;
    }
}
</string>
