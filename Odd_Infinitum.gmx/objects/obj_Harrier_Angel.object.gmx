          <argument>
            <kind>1</kind>
            <string>///Boss Action Manager

// States
switch (state)
{
    // Boss Stages
    case harrier_angel_states.intro:
        scr_harrier_angel_intro();
        break;
    case harrier_angel_states.phase_1_1:
        scr_harrier_angel_phase_1_1();
        break;
    case harrier_angel_states.phase_1_2:
        scr_harrier_angel_phase_1_2();
        break;
    case harrier_angel_states.phase_1_3:
        scr_harrier_angel_phase_1_3();
        break;
    case harrier_angel_states.phase_2:
        scr_harrier_angel_phase_2();
        break;
    case harrier_angel_states.defeat:
        scr_harrier_angel_defeat();
        break;
}

// If health is at 50%, transition to second phase
if (boss_health &lt;= 50) {state = harrier_angel_states.phase_2;}

// If boss health is 0, then enter defeated state
if (boss_health &lt; 0) {boss_health = 0;}
if (boss_health == 0) {state = harrier_angel_states.defeat;}
</string>
