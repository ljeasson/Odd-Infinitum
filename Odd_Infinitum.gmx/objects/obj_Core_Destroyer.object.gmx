        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Boss Action Manager

// States
switch (state)
{
    case core_destroyer_states.phase1:
        scr_core_destroyer_phase1();
        break;
    case core_destroyer_states.phase2:
        scr_core_destroyer_phase2();
        break;
    case core_destroyer_states.phase3:
        scr_core_destroyer_phase3();
        break;
}
    
//Randomize Laser and Missile Weakness
laser_weakness = irandom_range(0.25,2);
missile_weakness = irandom_range(6,10);

//Change sprtie based off boss health
if (boss_health &lt;= 100 &amp;&amp; boss_health &gt; 66) {state = core_destroyer_states.phase1;}
if (boss_health &lt;= 66 &amp;&amp; boss_health &gt; 33) {state = core_destroyer_states.phase2;}
if (boss_health &lt;= 33 &amp;&amp; boss_health &gt; 0) {state = core_destroyer_states.phase3;}

//When health is zero, boss is defeated
if (boss_health &lt; 0) {boss_health = 0;}
if (boss_health == 0)
{
    // Enable Defeated flag
    defeated = true;
}


// Advance onto screen if not defeated
// Retreat to offscreen if defeated
if (!defeated)
{ 
    move_towards_point(room_width - 420,room_height/2,entrance_speed);
    if (x == room_width - 420 &amp;&amp; y == room_height/2)
        hspeed = 0;    
}
else 
{
    // Stop Attack Audio
    audio_stop_sound(snd_Big_Laser_Charge_Up);
    audio_stop_sound(snd_Big_Laser_Fire);
    audio_stop_sound(snd_Laser_Blast);
    
    // Destroy Objects
    instance_destroy(core_destroyer_laser_controller);
    instance_destroy(obj_Core_Destroyer_Laser);
    instance_destroy(obj_Vortex);
    instance_destroy(obj_Blue_Ring);
    
    // Generate Explosions
    generate_explosions(200);
        
    // Advance Off-screen 
    move_towards_point(room_width+320,room_width/2,exit_speed);
    // Destroy When Off-screen
    if (x &gt; room_width + 310)
        instance_destroy();
}
</string>
