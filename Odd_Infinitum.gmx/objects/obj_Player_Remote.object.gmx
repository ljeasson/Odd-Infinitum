        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Player States
switch (remote_state)
{
    case remote_player_states.normal:
        // Normal movement and combat
        scr_remote_player_normal();
        break;
    case remote_player_states.damaged:
        // Player has taken damage
        scr_remote_player_damaged();
        break;
    case remote_player_states.dead:
        // Player is dead
        scr_remote_player_dead();
        break;
}

// Replenish overheat if overheat is less than 100
if (remote_energy &lt; 100) { remote_energy += global.overheat_recharge };

// If overheat is less than 0, set equal to 0
if (remote_energy &lt; 0) { remote_energy = 0 };
// If overheat is greater than 100, set equal to 100
if (remote_energy &gt; 100) { remote_energy = 100 };

// If health is greater than 100, set equal to 100
if (remote_health &gt; 100) { remote_health = 100 };
// If health is less than 0, change player state to dead
if (remote_health &lt;= 0) { remote_state = remote_player_states.dead };
</string>
          </argument>
        </arguments>
      </action>
    </event>
