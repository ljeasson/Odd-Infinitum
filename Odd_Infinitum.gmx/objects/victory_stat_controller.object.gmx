            break;
        case 1:
            scr_text_persistent("Missile", 1, UI_Font_Medium, left_column, room_height/2 - 48);
            scr_text_persistent(string(Missiles) + " X 1000", 1, UI_Font_Medium, right_column, room_height/2 - 48);
            stat_counter += 1;
            alarm[0] = 60;
            break;
        case 2:
            scr_text_persistent("No Overheat", 1, UI_Font_Medium, left_column, room_height/2);
            scr_text_persistent(string(No_Overheat), 1, UI_Font_Medium, right_column, room_height/2);
            stat_counter += 1;
            alarm[0] = 60;
            break;
        case 3:
            scr_text_persistent("No Damage", 1, UI_Font_Medium, left_column, room_height/2 + 48);
            scr_text_persistent(string(No_Damage), 1, UI_Font_Medium, right_column, room_height/2 + 48);
