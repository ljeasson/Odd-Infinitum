          <argument>
            <kind>1</kind>
            <string>/// Boss Fight Action Manager

// Once Intro is done and all intro objects (Fleet Commander and 3 Defectors)
// are deleted
if (instance_number(obj_Fleet_Commander_Daemeon_Intro) == 0 &amp;&amp;
    instance_number(obj_Defector_1_Orbit) == 0 &amp;&amp;
    instance_number(obj_Defector_2_Orbit) == 0 &amp;&amp;
    instance_number(obj_Defector_3_Orbit) == 0)
{
    // If there are no Defectors present
    if (instance_number(asset_get_index("obj_Defector_" + string(mini_boss_num+1) + "_Combat")) &lt; 1) 
    {
        // Create a Defector based on current value of 'mini_boss_num' 
        instance_create(room_width/2 + 256, room_height/2, asset_get_index("obj_Defector_" + string(mini_boss_num+1) + "_Combat"));    
    }  
}

// Once all 3 mini-bosses are defeated,
// spawn Fleet Commander Daemeon
if (mini_boss_num == 3 &amp;&amp; instance_number(obj_Fleet_Commander_Daemeon) &lt; 1)
{
    instance_create(room_width + 384, room_height/2, obj_Fleet_Commander_Daemeon);
    instance_destroy();
}
